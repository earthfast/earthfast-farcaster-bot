{
  "name": "earthfast-chatbot",
  "version": "1.0.0",
  "description": "https://github.com/neynarxyz/farcaster-examples/blob/main/gm-bot/",
  "main": "src/server.ts",
  "scripts": {
    "watch": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "start": "npm run build && bun run src/server.ts",
    "generate-bot-signer": "ts-node src/generateSignerForBot.ts",
    "test": "mocha -r ts-node/register 'test/**/*.ts'",
    "test:watch": "mocha -r ts-node/register 'test/**/*.ts' --watch",
    "lint": "eslint 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "fix": "npm run format && npm run lint -- --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.714.0",
    "@neynar/nodejs-sdk": "^2.0.5",
    "bun": "^1.1.38",
    "chatgpt": "^5.2.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.13.4",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "openai": "^4.76.1",
    "typescript": "^5.0.0",
    "viem": "^1.19.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "chai": "^4.3.4",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mocha": "^9.1.3",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
